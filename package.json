{
  "name": "clawpanel",
  "version": "4.1.0",
  "description": "ClawPanel — OpenClaw 智能管理面板，多通道接入、技能管理、定时任务、深度配置",
  "private": true,
  "scripts": {
    "dev:server": "cd server && npx tsx watch src/index.ts",
    "dev:web": "cd web && npm run dev",
    "build:server": "cd server && npx tsc",
    "build:web": "cd web && npm run build",
    "build": "npm run build:server && npm run build:web",
    "start": "cd server && node dist/index.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": ["clawpanel", "openclaw", "qq", "wechat", "telegram", "discord", "bot", "napcat", "onebot", "ai", "管理面板", "docker"],
  "author": "zhaoxinyi02",
  "license": "MIT"
}
